{
  "name": "asm68k",
  "displayName": "SNASM68k",
  "description": "SNASM68K syntax highlighter",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/HedroPedro/asm68k_extension.git"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "asm68k",
      "aliases": ["Motorola 68000", "asm68k"],
      "extensions": [".asm",".68k",".s"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "asm68k",
      "scopeName": "source.asm68k",
      "path": "./syntaxes/asm68k.tmLanguage.json"
    }]
  }
}
